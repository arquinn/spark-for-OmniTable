spark.driver.userClassPathFirst=true
spark.executor.userClassPathFirst=true
spark.executor.extraJavaOptions:-XX:+HeapDumpOnOutOfMemoryError -XX:HeapDumpPath=/tmp/heapDumps
spark.driver.extraJavaOptions:-XX:+HeapDumpOnOutOfMemoryError -XX:HeapDumpPath=/tmp/heapDumps

spark.driver.memory 2g
spark.executor.memory 2g